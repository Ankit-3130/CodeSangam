<div id="toolscontainer">
  <h5 style="font-size: 18px;">Tools</h5>
  <button class="tools" id="eraser" onclick="start_eraser()"><img id="icon" style="width:21px;height:21px" src="board icons/Eraser.png"></button>
  <button class="tools" id="bucketFill" onclick="bucket_fun()"><img id="icon" style="width:21px;height:21px" src="board icons/Eraser.png"></button>
</div>

    <div id="toolproperties">
      <h5 style="font-size: 18px; font-family: 'Times New Roman', Times, serif;">Tool Properties</h5>
      <div id="properties" style="width:85%">Stroke Size<input id="strokewidth" value="5" type="range" min="0.5"
          max="50" placeholder="Example: 1"></input></div>

      <div id="properties" style="width:85%">Pen Color<input id="strokecolor" value="#000000" type="color"></div>
      <div id="properties2" style="width:0%">Board Color<input id="boardcolor" value="#ffffff" type="color"></div>
    </div>

    <div id="actions">
      <h5 style="font-size: 18px;">Actions</h5>
      <button class="action" id="action1" onclick="clear_page(cntx)"><span class="tooltiptext"
          style="bottom:115%;left: 0%;">Clear Page</span><img id="icon" src="Clear.svg"></button>

      <button class="action" id="action8" onclick="undo()"><span class="tooltiptext"
          style="bottom:355%;left:0%;">Undo</span><img id="icon" src="Undo.svg"></button>
      <button class="action" id="action9" onclick="redo()"><span class="tooltiptext"
          style="bottom:335%;left:-100%;">Redo</span><img id="icon" src="Redo.svg"></button>
          <button class="action" id="action9" onclick="start_rect_drawing()"><span class="tooltiptext"
            style="bottom:335%;left:-100%;">Rect</span><img id="icon" src="rect.png" style="height: 15px;"></button>
            <button class="action" id="action9" onclick="start_circle_drawing()"><span class="tooltiptext"
              style="bottom:335%;left:-100%;">Circle</span><img id="icon" src="circle.png" style="height: 15px;"></button>
              <button class="action" id="action9" onclick="line()"><span class="tooltiptext"
                style="bottom:335%;left:-100%;">Triangle</span><img id="icon" src="triangle.png" style="height: 15px;"></button>
                <button class="action" id="action4" onclick="download_img()" alt="Download as .jpg"><span class="tooltiptext" style="bottom:115%;left:-100%;">Dowload Image</span><img id="icon" src="board icons/Capture.svg"></button>
          
          </div>


    </br>
