<div id="toolscontainer">
<h5 style="font-size: 18px;">Tools</h5>
<button class="tools" id="eraser" onclick="start_eraser()"><img id="icon" style="width:21px;height:21px" src="js/eraser.jpeg"></button>
<button class="tools" id="bucketFill" onclick="bucket_fun()"><img id="icon" style="width:21px;height:21px" src="js/bucket.jpeg"></button>
</div>

<div id="toolproperties">
    <h5 style="font-size: 18px; font-family: 'Times New Roman', Times, serif;">Tool Properties</h5>
    <div id="properties" style="width:85%">Stroke Size<input id="strokewidth" value="5" type="range" min="0.5"
        max="50" placeholder="Example: 1"></input></div>

    <div id="properties" style="width:85%">Pen Color<input id="strokecolor" value="#000000" type="color"></div>
    <div id="properties2" style="width:0%">Board Color<input id="boardcolor" value="#ffffff" type="color"></div>
  </div>

  <div id="actions">
    <h5 style="font-size: 18px;">Actions</h5>
    <button class="action" id="action1" onclick="clear_page(cntx)"><span class="tooltiptext"
