    <button class="action" id="action8" onclick="undo()"><span class="tooltiptext"
        style="bottom:355%;left:0%;">Undo</span><img id="icon" style="background-color: white;
        padding: 3px;" src="Undo.svg"></button>
    <button class="action" id="action9" onclick="redo()"><span class="tooltiptext"
        style="bottom:335%;left:-100%;">Redo</span><img id="icon" style="background-color: white;
        padding: 3px;" src="Redo.svg"></button>
        <button class="action" id="action9" onclick="start_rect_drawing()"><span class="tooltiptext"
          style="bottom:335%;left:-100%;">Rect</span><img id="icon" src="rect.png" style="height: 15px;"></button>
          <button class="action" id="action9" onclick="start_circle_drawing()"><span class="tooltiptext"
            style="bottom:335%;left:-100%;">Circle</span><img id="icon" src="circle.png" style="height: 15px;"></button>
            <button class="action" id="action9" onclick="line()"><span class="tooltiptext"
              style="bottom:335%;left:-100%;">Triangle</span><img id="icon" src="triangle.png" style="height: 15px;"></button>
              <button class="action" id="action4" onclick="download_img()" alt="Download as .jpg"><span class="tooltiptext" style="bottom:115%;left:-100%;">Download Image</span><img id="icon" style="width: 24px;" src="js/download.png"></button>
        
        </div>


  </br>
